<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.shangcheng.mapper.AccountMapper">


	<select id="getAccountList" resultType="com.shangcheng.entity.Account">

		select * from t_account

	</select>

	<select id="getAccountById" resultType="com.shangcheng.entity.Account">
		select * from t_account where accountId = #{AccountId}


	</select>
</mapper>